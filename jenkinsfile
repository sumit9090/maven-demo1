node{


stage('SCM Checkout')
{
  git 'https://github.com/sumit9090/maven-demo1'
}

stage('Compile-package')
{
def mvnHome=tool name: 'Default', type: 'maven'
  echo "{$mvnHome}/bin/mvn package"
}

stage('Clean install')
{
def mvnHome=tool name: 'Default', type: 'maven'
  echo "{$mvnHome}/bin/mvn install"
}
}
